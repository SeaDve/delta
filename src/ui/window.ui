<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="DeltaWindow" parent="AdwApplicationWindow">
    <property name="width-request">480</property>
    <property name="height-request">320</property>
    <property name="default-width">480</property>
    <property name="default-height">320</property>
    <property name="content">
      <object class="GtkOverlay">
        <property name="child">
          <object class="AdwToastOverlay" id="toast_overlay">
            <property name="child">
              <object class="GtkStack" id="page_stack">
                <property name="transition-type">over-up-down</property>
                <child>
                  <object class="GtkBox" id="main_page">
                    <child>
                      <object class="GtkWindowHandle">
                        <property name="child">
                          <object class="GtkBox">
                            <property name="orientation">vertical</property>
                            <child>
                              <object class="GtkStackSwitcher">
                                <property name="vexpand">True</property>
                                <property name="stack">view_stack</property>
                                <property name="orientation">vertical</property>
                                <style>
                                  <class name="view-switcher"/>
                                </style>
                              </object>
                            </child>
                            <child>
                              <object class="GtkImage" id="gps_status_icon">
                                <property name="margin-top">6</property>
                                <property name="margin-bottom">6</property>
                                <property name="icon-name">globe-symbolic</property>
                              </object>
                            </child>
                          </object>
                        </property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkStack" id="view_stack">
                        <property name="transition-type">slide-up-down</property>
                        <property name="vexpand">True</property>
                        <property name="hexpand">True</property>
                        <child>
                          <object class="GtkStackPage">
                            <property name="icon-name">view-list-symbolic</property>
                            <property name="child">
                              <object class="GtkListBox" id="peer_list_box">
                                <property name="margin-start">12</property>
                                <property name="margin-end">12</property>
                                <property name="margin-top">12</property>
                                <property name="margin-bottom">12</property>
                                <property name="selection-mode">none</property>
                                <property name="valign">start</property>
                                <style>
                                  <class name="boxed-list"/>
                                </style>
                              </object>
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkStackPage">
                            <property name="icon-name">map-symbolic</property>
                            <property name="child">
                              <object class="DeltaMapView" id="map_view">
                              </object>
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkStackPage">
                            <property name="icon-name">send-symbolic</property>
                            <property name="child">
                              <object class="GtkGrid">
                                <property name="margin-start">18</property>
                                <property name="margin-end">18</property>
                                <property name="margin-top">18</property>
                                <property name="margin-bottom">18</property>
                                <property name="column-homogeneous">True</property>
                                <property name="row-homogeneous">True</property>
                                <property name="row-spacing">18</property>
                                <property name="column-spacing">18</property>
                                <child>
                                  <object class="GtkButton" id="sos_button">
                                    <property name="child">
                                      <object class="GtkBox">
                                        <property name="valign">center</property>
                                        <property name="orientation">vertical</property>
                                        <property name="spacing">12</property>
                                        <child>
                                          <object class="GtkImage">
                                            <property name="icon-name">rescue-symbolic</property>
                                            <property name="icon-size">large</property>
                                            <style>
                                              <class name="error"/>
                                            </style>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkLabel">
                                            <property name="label">SOS</property>
                                            <style>
                                              <class name="heading"/>
                                            </style>
                                          </object>
                                        </child>
                                      </object>
                                    </property>
                                    <style>
                                      <class name="card"/>
                                    </style>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkButton" id="hazard_button">
                                    <property name="child">
                                      <object class="GtkBox">
                                        <property name="valign">center</property>
                                        <property name="orientation">vertical</property>
                                        <property name="spacing">12</property>
                                        <child>
                                          <object class="GtkImage">
                                            <property name="icon-name">dialog-warning-symbolic</property>
                                            <property name="icon-size">large</property>
                                            <style>
                                              <class name="warning"/>
                                            </style>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkLabel">
                                            <property name="label">Hazard</property>
                                            <style>
                                              <class name="heading"/>
                                            </style>
                                          </object>
                                        </child>
                                      </object>
                                    </property>
                                    <style>
                                      <class name="card"/>
                                    </style>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkButton" id="yielding_button">
                                    <property name="child">
                                      <object class="GtkBox">
                                        <property name="valign">center</property>
                                        <property name="orientation">vertical</property>
                                        <property name="spacing">12</property>
                                        <child>
                                          <object class="GtkImage">
                                            <property name="icon-name">thumbs-up-symbolic</property>
                                            <property name="icon-size">large</property>
                                            <style>
                                              <class name="accent"/>
                                            </style>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkLabel">
                                            <property name="label">Yielding</property>
                                            <style>
                                              <class name="heading"/>
                                            </style>
                                          </object>
                                        </child>
                                      </object>
                                    </property>
                                    <style>
                                      <class name="card"/>
                                    </style>
                                  </object>
                                </child>
                              </object>
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkStackPage">
                            <property name="icon-name">settings-symbolic</property>
                            <property name="child">
                              <object class="DeltaSettingsView" id="settings_view"/>
                            </property>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="DeltaCallPage" id="call_page"/>
                </child>
                <child>
                  <object class="DeltaCrashedPage" id="crashed_page"/>
                </child>
                <child>
                  <object class="DeltaPlacePage" id="place_page"/>
                </child>
              </object>
            </property>
          </object>
        </property>
        <child type="overlay">
          <object class="GtkRevealer" id="listening_overlay_revealer">
            <property name="visible">False</property>
            <property name="transition-type">crossfade</property>
            <child>
              <object class="DeltaListeningOverlay" id="listening_overlay">
                <property name="margin-start">24</property>
                <property name="margin-end">24</property>
                <property name="margin-top">24</property>
                <property name="margin-bottom">24</property>
                <style>
                  <class name="card"/>
                </style>
              </object>
            </child>
          </object>
        </child>
      </object>
    </property>
  </template>
</interface>
